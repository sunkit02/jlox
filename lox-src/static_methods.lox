class Math {
    static square(n) {
        return n * n;
    }

    // n % m
    static mod(n, m) {
        while (n > m) {
            n = n - m;
        }
        return n;
    }

    static abs(n) {
        if (n < 0) return -n;
        return n;
    }
}

var math = Math();
print math.square(1000);

print Math.square(2);
print Math.abs(-100);
print Math.abs(100);
print Math.mod(7, 2);